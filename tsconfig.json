{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "strict": true,
      "jsx": "react",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["resources/js/**/*"],
    "baseUrl": "resources/js/",                                  /* Specify the base directory to resolve non-relative module names. */
    "paths": {
     "@/*": ["*"]
  }
}